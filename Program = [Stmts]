Program = [Stmts]

Stmts = 2Arg | 3Arg

2Arg = [Load, Store] > Register > Comma > Expression
3Arg = [Add, Sub] > Register > Comma > Expression > Comma > Expression


Expression = 
    | LiteralExpression
    | Direct > Paren > LiteralExpression > Paren
    | Indirect > Paren > LiteralExpression > Paren

LiteralExpression = 
    | Math
    | Register
    | Name 
    | Number

Math = [Number, Name] > [+, -, *, /, &, |] > [Number, Name]




ld r1, 2